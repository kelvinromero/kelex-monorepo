type Query {
    episodes: [Episode]
    episodesByTitle(title: String!): [Episode]
    findEpisodeByFuzzyTitle(title: String!): [Episode]
    episodeById(id: ID!): Episode

#    podcasts: [Podcast]
#    podcastById(id: ID!): Podcast
}

type Episode {
    id: ID!
    title: String!
    description: String!
    mediaUrl: String!
